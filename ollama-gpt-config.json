{
  "name": "Ollama GPT Local Engine",
  "description": "本地 Ollama GPT 引擎配置",
  "version": "1.0.0",
  "engine": {
    "provider": "ollama",
    "baseURL": "http://localhost:11434",
    "model": "gpt-oss:20b",
    "apiVersion": "v1"
  },
  "settings": {
    "temperature": 0.7,
    "max_tokens": 4096,
    "top_p": 0.9,
    "stream": false,
    "timeout": 30000
  },
  "endpoints": {
    "generate": "/api/generate",
    "chat": "/api/chat",
    "models": "/api/tags",
    "health": "/api/version"
  },
  "features": {
    "streaming": true,
    "thinking": true,
    "context": true,
    "system_prompt": true
  },
  "model_info": {
    "name": "gpt-oss:20b",
    "size": "13GB",
    "parameters": "20B",
    "type": "text-generation",
    "capabilities": [
      "text-generation",
      "conversation",
      "code-generation",
      "reasoning"
    ]
  }
}
